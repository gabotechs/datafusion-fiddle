{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "framework": null,
  "devCommand": "vite --port $PORT",
  "git": {
    "lfs": true
  },
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-rust@4.0.9",
      "excludeFiles": "**",
      "includeFiles": "api/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
