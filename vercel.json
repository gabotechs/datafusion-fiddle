{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "framework": null,
  "devCommand": "vite --port $PORT",
  "git": {
    "lfs": true
  },
  "functions": {
    "api/**/*.rs": {
      "runtime": "vercel-rust@4.0.9",
      "excludeFiles": "{dist,*.cache,node_modules,public,src,target}/**",
      "includeFiles": "api/parquet/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
